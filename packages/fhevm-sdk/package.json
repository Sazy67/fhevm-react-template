{
  "name": "@fhevm-sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./src/core/index.ts",
      "default": "./dist/core/index.js"
    },
    "./storage": {
      "types": "./src/storage/index.ts",
      "default": "./dist/storage/index.js"
    },
    "./types": {
      "types": "./src/fhevmTypes.ts",
      "default": "./dist/fhevmTypes.js"
    },
    "./react": {
      "types": "./src/react/index.ts",
      "default": "./dist/react/index.js"
    },
    "./universal": {
      "types": "./src/universal/index.ts",
      "default": "./dist/universal/index.js"
    },
    "./universal/core": {
      "types": "./src/universal/core/index.ts",
      "default": "./dist/universal/core/index.js"
    },
    "./universal/storage": {
      "types": "./src/universal/storage/index.ts",
      "default": "./dist/universal/storage/index.js"
    },
    "./universal/utils": {
      "types": "./src/universal/utils/index.ts",
      "default": "./dist/universal/utils/index.js"
    },
    "./universal/react": {
      "types": "./src/universal/adapters/react/index.ts",
      "default": "./dist/universal/adapters/react/index.js"
    },
    "./universal/vue": {
      "types": "./src/universal/adapters/vue/index.ts",
      "default": "./dist/universal/adapters/vue/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "cli": "node dist/cli/index.js"
  },
  "bin": {
    "fhevm-sdk": "dist/cli/index.js"
  },
  "dependencies": {
    "idb": "^8.0.3",
    "commander": "^12.1.0"
  },
  "peerDependencies": {
    "@fhevm/mock-utils": "^0.1.0",
    "@zama-fhe/relayer-sdk": "^0.2.0",
    "ethers": "^6.13.4",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "~18.19.50",
    "@types/react": "~19.0.7",
    "@vitest/coverage-v8": "2.1.9",
    "@zama-fhe/relayer-sdk": "0.2.0",
    "ethers": "^6.13.7",
    "fake-indexeddb": "~6.0.0",
    "jsdom": "^27.0.0",
    "react": "~19.0.0",
    "typescript": "~5.8.2",
    "vitest": "~2.1.8"
  }
}
